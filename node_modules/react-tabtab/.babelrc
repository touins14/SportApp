{
  "presets": ["react", "es2015", "stage-0"],
  "plugins": [
    [
      "transform-decorators-legacy",
      "transform-class-properties",
      "add-module-exports"
    ]
  ],
  "env": {
    "development": {
      "presets": ["react-hmre"],
      "plugins": [
        "add-module-exports"
      ]
    },
    "production": {
      "plugins": [
        "transform-react-remove-prop-types",
        "add-module-exports"
      ]
    }
  }
}
